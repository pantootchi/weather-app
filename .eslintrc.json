{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "airbnb", "prettier"],
    "plugins": ["prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-param-reassign": "off",
        "no-use-before-define": "warn"
    }
}